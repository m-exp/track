git status
git add
git commit -m "message"

push
	ex: git push origin master

git init: starting git in a new directory
git clone [url] [newname]
git status 
git status -s
.gitignore 
git diff
git diff --staged
git commmit  -a #: all files that are changed (and are tracked)
git rm
git rm --cached <file> : remove from staging area.
git log options:
	 -p -<number> --stat --pretty 
	--pretty=oneline|short|full|fuller|=format:"%foo"
	--graph
limiting log:
	-<n>, --since, --after, --until, --before, --author, --committer, --grep, -Sfunction_name

ch3

git commit --amend : ammend a commit
git reset HEAD <file>  : unstage a file
git checkout -- <file> : Unod the changes made in the file since the last commit

